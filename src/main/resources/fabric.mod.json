{
	"schemaVersion": 1,
	"id": "item-frame-click-through",
	"version": "${version}",
	"name": "Item Frame Click Through",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/item-frame-click-through/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.zerotask.minecraft.clickthrough.ItemFrameClickThrough",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "net.zerotask.minecraft.clickthrough.ItemFrameClickThroughClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"item-frame-click-through.mixins.json",
		{
			"config": "item-frame-click-through.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.2",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}